{
	"name": "wpdecoupled-site",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host 127.0.0.1",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node build",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:format": "prettier --plugin-search-dir . --check . ",
		"test:lint": "eslint .",
		"lint": "yarn test:lint --fix",
		"format": "yarn test:format --write",
		"generate": "houdini pull-schema && houdini generate"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/gtag.js": "^0.0.19",
		"@typescript-eslint/eslint-plugin": "^7.7.1",
		"@typescript-eslint/parser": "^7.7.1",
		"autoprefixer": "^10.4.13",
		"eslint": "^9.1.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"houdini": "^1.1.3",
		"houdini-svelte": "^1.1.3",
		"postcss": "^8.4.14",
		"postcss-custom-media": "^10.0.4",
		"postcss-load-config": "^5.0.3",
		"postcss-nesting": "^12.1.2",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.15",
		"svelte-check": "^3.0.2",
		"svelte-preprocess": "^5.1.4",
		"tslib": "^2.4.1",
		"typescript": "^5.4.5",
		"vite": "^5.2.10"
	},
	"type": "module",
	"dependencies": {
		"@js-temporal/polyfill": "^0.4.3",
		"@sentry/node": "^7.47.0",
		"@sentry/svelte": "^7.47.0",
		"@sentry/tracing": "^7.47.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"feed": "^4.2.2",
		"http-status-codes": "^2.2.0",
		"open-props": "^1.5.3"
	},
	"engines": {
		"node": ">=20.0.0 <21.0.0"
	},
	"packageManager": "yarn@1.22.19"
}